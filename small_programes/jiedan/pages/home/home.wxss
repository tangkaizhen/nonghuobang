/* pages/home/home.wxss */
page{
  height:100%;
}
.empty_custom {
  border-bottom: none;
  margin-bottom:137rpx;
}
.search_box{
   display: flex;
   border-bottom: 1rpx solid #eee;
   padding: 40rpx 0;
   justify-content: space-around;
   width:100%;
    position:fixed;
    top:130rpx;
    z-index:1;
    background:#fff;

 }
 .search_inner_box{
   width: 500rpx;
   height: 66rpx;
   background: #f5f5f5;
   border-radius: 33rpx;
   display: flex;
 }
 .search_txt{
   font-size: 24rpx;
   color: #333;
   margin:18rpx 10rpx 0rpx 30rpx;

 }
 .search_img{
   width: 24rpx;
   height: 24rpx;
   position:relative;
   top:21rpx;

 }
 .search_right_box{
   display: flex
 }
 .search_type{
   width: 30rpx;
   height: 30rpx;
   margin-top:10rpx;
   margin-right:21rpx;

 }
 .search_filtrate_box{
   display: flex;
 }
 .search_filtrate_txt{
   font-size: 24rpx;
   color: #333;
   margin-right:11rpx;
   margin-top:15rpx;

 }
 .search_img_down{
   width: 13rpx;
   height: 8rpx;
   position:relative;
   bottom:-24rpx;
 }

.filtrate_box{
  padding: 20rpx 40rpx;
  position:fixed;
  width:100%;
  background:#fff;
  z-index:10000;
  top:234rpx;
}
.order_list_box{
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between
}
.order_list_box{
  /* border-bottom: 1rpx solid #eee */
}
.order_list{
  font-size: 22rpx;
  color: #ccc;
  border: 2rpx solid #ccc;
  height: 48rpx;
  width: 176rpx;
  border-radius:22rpx; 
  text-align: center;
  line-height: 48rpx;
  margin-bottom: 20rpx
}
.order_select{
  background: #333;
  color: #fff
}
.borderLine{
  width: 100%;
  background: #eee;
  height: 1rpx
}
.order_cancel_assign_box{
  font-size:24rpx;
  color: #333; 
  padding-bottom:30rpx;

}
.order_cancel_box,.order_assign_box{
  display: flex;
  padding-left: 30rpx;
  margin-top: 30rpx;
  font-size: 24rpx
}
.order_cancel_select_img,.order_assign_select_img{
  width: 60rpx;
  height: 33rpx;  
  margin-left: 20rpx;
}

.order_btn_box{
  font-size: 30rpx;
  color: #6cd986;
  display: flex;
  justify-content: space-around;
  padding-top: 40rpx;
}
.order_btn_cancel{
  line-height:60rpx;
  height:60rpx;

}
.order_btn_confirm{
  border: 1rpx solid #6cd986;
  height: 60rpx;
  line-height: 60rpx;
  width: 300rpx;
  text-align: center;
  border-radius: 40rpx;
}


/* 下面是订单详情 */
.order_box{
    width: 100%;
    background: #eee;
    padding: 20rpx;
}
.order_content{
    background: #fff;
    margin-bottom: 20rpx;
    border-radius: 10rpx;
    padding: 20rpx 35rpx 30rpx;
    text-align: center;

}
.order_content.index{
  background:url(data:image/png;base64,) no-repeat right bottom,#fff;
  background-size: 12%
}
.order_top{
  margin-bottom: 30rpx;  
  display: flex;
  justify-content: space-between
}
.order_number{
  display: flex;
  align-items: center
}
.order_logo{
    margin-right: 10rpx;
    width: 37rpx;
    height: 37rpx;
}
.order_number_val{
    font-size: 24rpx;
    color: #999;
    position:relative;
}
.order_status{
    font-size: 28rpx;
    color: #6cd986;
}
.order_status.index{
  color: #999;
}
.order_center.index{
  background:url(data:image/png;base64,) no-repeat 90% center;
  background-size: 20%
}
.order_header{
    font-size: 36rpx;
    color: #666;
}
.order_header_unit{
  font-size: 25rpx;
}
.order_position{
    font-size: 24rpx;
    color: #666;
    margin-top: 30rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}
.order_date{
    font-size:22rpx;
    margin-top: 20rpx;
    color: #666;
}
.order_one_btn{
    font-size:32rpx;
    width: 60%;
    margin-left: 20%;
    color: #fff;
    text-align: center;
    background: linear-gradient(to right, #6cd986, #22d2ac);
    height: 60rpx;
    line-height: 60rpx;
    border-radius: 30rpx;
    margin-top: 32rpx;
}
.order_one_btn.index{
    background: #fff;
    color: #6cd986;
    border: 2rpx solid #6cd986;
}

.order_two_btns_box{
  margin-top: 26rpx;
  display: flex;
  justify-content: space-around
}
.order_two_btns_first{
  font-size: 32rpx;
  color: #6cd986;
  width: 50%;
  text-align: center;
  height: 54rpx;
  line-height: 54rpx;
}
.order_two_btns_second{
    font-size:32rpx;
    width: 40%;
    color: #fff;
    text-align: center;
    background:linear-gradient(to right, #6cd986, #22d2ac);
    height: 54rpx;
    line-height: 54rpx;
    border-radius: 27rpx;
}

.order_assign_helper_box{
    border-top: 1rpx solid #eee;
    margin-top: 24rpx;
    padding-top: 24rpx;
    display: flex;
    justify-content: space-between;
}
.order_assign_helper_left{

}
.order_assign_helper_img{
  width:34rpx;
  height:34rpx;
  margin-right:15rpx;
  position:relative;
  top:7rpx;
}
.order_assign_helper_name{
font-size:32rpx;
    color: #999;
}
.order_assign_helper_right{
font-size: 32rpx;
    color: #999;
}
